#Add an item
tp=(4,6,2,8,3,1)
print(tp)
x=(9,)
tp=tp+x
print(tp)
tp=tp[:5]+(15,20,25)+tp[:5]
print(tp)
list=list(tp)
list.append(30)
tp=tuple(list)
print(tp)


#Length of a Tuple
n=input()
tp=tuple(n)
print(tp)
print(len(tp))


#Reverse of a tuple
n=input()
lst=list(n)
lst.reverse()
tp=tuple(lst)
print(tp)


#Repeated element in a tuple
tp=(2,4,5,6,2,3,4,4,7)
print(tp)
n=int(input())
print(tp.count(n))
